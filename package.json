{
  "name": "nanocurrency",
  "description": "Toolkit for the Nano cryptocurrency",
  "version": "0.0.0-development",
  "author": {
    "name": "Marvin ROGER",
    "email": "dev@marvinroger.fr",
    "url": "https://www.marvinroger.fr"
  },
  "browser": "dist/nanocurrency.umd.js",
  "bugs": "https://github.com/marvinroger/nanocurrency-js/issues",
  "dependencies": {},
  "devDependencies": {
    "rollup": "^0.55.5",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "semantic-release": "^12.4.1",
    "travis-deploy-once": "^4.3.4"
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/marvinroger/nanocurrency-js",
  "keywords": [
    "currency",
    "nano",
    "pow",
    "work"
  ],
  "license": "GPL-3.0",
  "main": "dist/nanocurrency.cjs.js",
  "module": "dist/nanocurrency.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/marvinroger/nanocurrency-js.git"
  },
  "scripts": {
    "build": "yarn build:native && yarn build:js",
    "build:js": "rollup -c",
    "build:native": "docker run --rm -v $(pwd):/src trzeci/emscripten emcc -o native.js -s WASM=1 -s MODULARIZE=1 -s SINGLE_FILE=1 -s 'EXTRA_EXPORTED_RUNTIME_METHODS=[\"cwrap\"]' -O3 --closure 1 --llvm-lto 3 src/native/functions.cpp src/native/blake2/ref/blake2b-ref.c",
    "semantic-release": "semantic-release",
    "test": "node test/test.js",
    "travis-deploy-once": "travis-deploy-once"
  }
}
